@model IEnumerable<Web_CuoiKy.Models.Loai_SP>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_layout.cshtml";
}
<h4><center>Danh sách loại sản phẩm</center></h4>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MALOAI)
            
        </th>
        <th>
           
            @Html.DisplayNameFor(model => model.TENLOAI)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.MALOAI)
       
    </td>
    <td>
        
        @Html.DisplayFor(modelItem => item.TENLOAI)
    </td>
    <td>
        @Html.ActionLink("Thêm", "Create") |
        @Html.ActionLink("Sửa", "Edit", new { id = item.MALOAI }) |
        @Html.ActionLink("Xóa", "Delete", new { id = item.MALOAI })
    </td>
</tr>
}

</table>
